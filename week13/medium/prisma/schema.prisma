
generator client {
  provider = "prisma-client-js"
  engineType = "binary"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id Int @id @default(autoincrement())
  name String 
  userName String @unique
  password String
  blogs Blog[]
}

model Blog {
    id Int @id @default(autoincrement())
    authorId Int
    content String
    title String
    published Boolean @default(false)
    author User @relation(fields: [authorId] ,references: [id])
}
